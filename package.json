    "gatsby-transformer-sharp": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },

  "devDependencies": {
    "prettier": "^2.8.8"
  },

  "keywords": [
