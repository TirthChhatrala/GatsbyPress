{
  "name": "gatsbypress",
  "private": true,
  "description": "A modern Gatsby blog with Markdown, SEO, pagination, search, dark mode, tags, RSS, and custom styling.",
  "version": "1.0.0",
  "author": "Tirth Chhatrala",
  "license": "MIT",

  "engines": {
    "node": "18.x"
  },

  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\""
